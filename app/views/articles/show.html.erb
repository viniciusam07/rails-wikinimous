<h2><%= @article.title %></h2>
<p><%= @article.created_at %> </p>
<p><%= @article.content %> </p>
<button class="btn btn-primary"><%= link_to "Edit Article", edit_article_path %></button>


<p><%= link_to "Delete Post", article_path(@article), method: :delete, data: { confirm: "Are you sure?" }%></p>

<p><%= link_to "Back to All Posts", articles_path(@articles) %></p>
